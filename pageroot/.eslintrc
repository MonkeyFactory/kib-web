{
  "extends": "eslint:recommended",
  "plugins": ["angular"],
  "env": {
    "browser": true,	
    "jasmine": true
  },
  "globals": {
    "angular": true,
    "module": true,
    "inject": true,
	"moment": true
  },
  "rules": {
	"angular/controller-as": 0,
	"angular/controller-as-route": 0,
	"angular/di": 0,
	"no-mixed-spaces-and-tabs": 0
  }
}
